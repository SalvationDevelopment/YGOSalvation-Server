{
  "name": "public",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:docker": "yarn docker:down && yarn docker:up && yarn docker:logs && yarn docker:down",
    "docker:up": "docker run --name  frontend -d -p 3000:3000 frontend-public",
    "docker:down": "docker rm --force frontend",
    "docker:logs": "docker logs -f --tail 10 frontend",
    "build": "next build",
    "build:docker": "yarn docker:down && docker build -t frontend-public .",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "next": "11.1.2",
    "nookies": "^2.5.2",
    "pm2": "^5.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2"
  }
}
